function showmore_boss(){
	document.getElementById("showmore_boss").style.display = "none";
	document.getElementById("hidemore_boss").style.display = "block";
	document.getElementById("block_boss").style.height = "";
	document.getElementById("block_boss").style.overflow = "visible";
	document.getElementById("block_boss").innerHTML =  __call$(document.getElementById("block_boss").innerHTML,"replace",[/lz_src/g,"src"]) ;	
}

function hidemore_boss(){
	document.getElementById("hidemore_boss").style.display = "none";
	document.getElementById("showmore_boss").style.display = "block";
	document.getElementById("block_boss").style.height = "520px";
	document.getElementById("block_boss").style.overflow = "hidden";
}

function showmore_zm3(){
	document.getElementById("showmore_zm3").style.display = "none";
	document.getElementById("block_0").style.height = "";
	document.getElementById("block_0").style.overflow = "visible";
	document.getElementById("block_0").innerHTML =  __call$(document.getElementById("block_0").innerHTML,"replace",[/lz_src/g,"src"]) ;
}


function show_tab(i){
document.getElementById("showmore_zm3").style.display = "none";
if(i==0){
		if(document.getElementById('block_0').style.height=='685px'){
		document.getElementById('showmore_zm3').style.display='block';
	}
}

var block_id,li_id;
for (j=0;j<=7;j++){
li_id = "li_"+j;
block_id = "block_"+j;
var k = j+1;
if (j==i){
document.getElementById(block_id).style.display="block";
document.getElementById(li_id).className='on';
if(i!=0){
document.getElementById(block_id).innerHTML =  __call$(document.getElementById(block_id).innerHTML,"replace",[/lz_src/g,"src"]) ;
}
}
else{
document.getElementById(block_id).style.display="none";
document.getElementById(li_id).className='on1';
}
}
}

 function show_tab_zm2(i){
    var block_id,li_id;
    for (j=1;j<=5;j++){
      li_id = "zm2_li_"+j;
      block_id = "zm2_block_"+j;
      if (j==i){
        document.getElementById(block_id).style.display="block";
        document.getElementById(li_id).className='on';
      }
      else{
        document.getElementById(block_id).style.display="none";
        document.getElementById(li_id).className='';
      }
    }
  }

