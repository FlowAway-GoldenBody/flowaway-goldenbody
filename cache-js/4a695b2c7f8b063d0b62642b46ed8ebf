function switchpage(totalpage,controller,page){
	var prepage = page-1;
	if(prepage<1){prepage=totalpage}
	var prestring =  __get$(document.getElementById(controller+"_pre"),"href") ;
	prestring =  __call$(prestring,"replace",[/,[0-9]+\)+/g,","+prepage+"\)"]) ;
	 __set$(document.getElementById(controller+"_pre"),"href",prestring) ;
	
	var nextpage = page+1;
	if(nextpage>totalpage){nextpage=1}
	var nextstring =  __get$(document.getElementById(controller+"_nex"),"href") ;
	nextstring =  __call$(nextstring,"replace",[/,[0-9]+\)+/g,","+nextpage+"\)"]) ;
	 __set$(document.getElementById(controller+"_nex"),"href",nextstring) ;
	
	for(var i=1;i<=totalpage;i++){
		if(i==page){	
			document.getElementById(controller+i).style.display = "block";
			document.getElementById(controller+i).innerHTML =  __call$(document.getElementById(controller+i).innerHTML,"replace",[/lz_src/g,"src"]) ;
		}else{
			document.getElementById(controller+i).style.display = "none";
		}
	}
}
