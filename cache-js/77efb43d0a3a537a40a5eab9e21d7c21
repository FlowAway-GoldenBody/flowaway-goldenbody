 $("#dkey").val(default_c1);
 var jz= __get$(dindex,kn) ;
 $("#dtab  a").attr("class","");
 $("#dtab  a").eq(jz).attr("class","on");
  if(kn==0){
	 daojulist=$("#daojulist li");
  }else{
	daojulist=$("#daojulist li").filter("[rel="+daojutype+"]");  
  }
 daojumore();
}

function searchdaoju(){
  var keyword=$("#dkey").val();
  var keyword=$.trim(keyword);
  if(keyword=='' || keyword==default_c){
	 alert(default_c1);$("#dkey").focus();
	 return false;
  }
  $("#dtab  a").attr("class","");
 daojulist=$("#daojulist li").filter("[rel1*='"+keyword+"']");
 if(daojulist.length<=0){alert("您搜索的道具不存在，请输入正确的道具名称，才能搜索到哦！");$("#dkey").focus();return false;}
 $("#dtab li a").attr("class","");
  daojumore();
}

