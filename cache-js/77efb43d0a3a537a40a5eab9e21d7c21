$(".z5tab li").click(function(){
	var tab=$(this).attr("rel");
	for(var i=1;i<=4;i++){
	  $("#zmxy5_tab"+i).hide();
	}
	$("#zmxy5_tab"+tab).show();
	var n=tab-1;
	$(".z5tab li").removeClass("on");
	$(".z5tab li").eq(n).addClass("on");
});
$(".z5tab li").hover(function(){
	var tab=$(this).attr("rel");
	for(var i=1;i<=4;i++){
	  $("#zmxy5_tab"+i).hide();
	}
	$("#zmxy5_tab"+tab).show();
	var n=tab-1;
	$(".z5tab li").removeClass("on");
	$(".z5tab li").eq(n).addClass("on");
});

