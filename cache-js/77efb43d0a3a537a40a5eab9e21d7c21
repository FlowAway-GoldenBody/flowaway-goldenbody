var daojunp=1;
var daojutype=0;
var dindex=new Array();
var default_c1='请输入你要查找的道具';
var daojulist=$("#daojulist li");
dindex[0]=0;
dindex[99]=1;
dindex[41]=2;
dindex[42]=3;
dindex[43]=4;
dindex[44]=5;
dindex[45]=6;
dindex[46]=7;
function daojumore(){
   var s1=daojunp*45;
   var e1=s1+45;
   daojunp=daojunp+1;
  var len=daojulist.length;
  if(len>45){$("#daojumore").css("display","");}else{$("#daojumore").css("display","none");}
  if(len<e1){e1=len;$("#daojumore").css("display","none");}
  $("#daojulist li").css("display","none");
  for(var j=0;j<e1;j++){
    daojulist.eq(j).show().find("img[lz_src]").attr("src",function(){
	   var src=$(this).attr("lz_src");
	   $(this).removeAttr("lz_src");
	   return src;
	});
  }
}
