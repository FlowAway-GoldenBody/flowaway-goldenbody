//轮播图功能
	var nscroll=0;var tt;
	function scrolly(){
	 $(".jsTabs a").attr("class","");
	 $(".jsTabs a").eq(nscroll).attr("class","on");
	 $(".showimg").css("display","none");
	 $(".showimg").eq(nscroll).css("display","");
	 nscroll=nscroll+1;
	 if(nscroll>=4){
	  nscroll=0;
	 }
	 tt=setTimeout("scrolly()",4000);
	}
	scrolly();
	function curs(k){
	 nscroll=k;
	 $(".jsTabs a").attr("class","");
	 $(".jsTabs a").eq(nscroll).attr("class","on");
	 $(".showimg").css("display","none");
	 $(".showimg").eq(nscroll).css("display","");  
	 clearTimeout(tt);
	}

function func_scroll(id,flag){
	var temp_scroll = $("#"+id);
	var li_length = $(temp_scroll).find("li").length;
	if(flag=="pre"){
		  var cli=temp_scroll.find("li:lt(2)");
		  cli.remove();
		  cli.appendTo(temp_scroll);
	}else{
		  var cli=temp_scroll.find("li:gt("+(li_length-3)+")")
		  cli.remove();
		  cli.prependTo(temp_scroll);
	}
	temp_scroll.find("img[lz_src]").attr("src",function(){
		var imgurl=$(this).attr("lz_src");
		$(this).removeAttr("lz_src");
		return imgurl;
	});
}

