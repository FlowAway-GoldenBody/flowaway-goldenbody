function jsgl(kn){
  $("#jsnav li").attr("class","");
  $("#jsnav li").eq(kn).attr("class","on");
  $("#gjz").attr("href", __get$(jsg,kn) );
  $("#gjz1").attr("href", __get$(jsg,kn) );
  $("#gjz").html( __get$(jsg1,kn) );
 // $("#gjz1").html(jsg2[kn]);  
  $(".js-don").css("display","none");
  $(".js-don").eq(kn).find("img[lz_src]").attr("src",function(){
	var src=$(this).attr("lz_src");
	$(this).removeAttr("lz_src");
	return src;
  });
  $(".js-don").eq(kn).css("display","");
}

function jtab(k1,k2){
  var obj1='#tab-bd'+k1+" ul";
  var obj2='#jj'+k1;
  var obj3="#jsb1-"+k1+" a";
  $(obj1).hide().eq(k2).show().find("img[lz_src]").attr("src",function(){var src=$(this).attr("lz_src");$(this).removeAttr("lz_src");return src;});
  $(obj3).hide().eq(k2).show().find("img[lz_src]").attr("src",function(){var src=$(this).attr("lz_src");$(this).removeAttr("lz_src");return src;});
  if(k2==1){
    $(obj2).find("a").eq(1).addClass("on");
	$(obj2).find("a").eq(0).removeClass("on");
  } 
   if(k2==0){
    $(obj2).find("a").eq(0).addClass("on");
	$(obj2).find("a").eq(1).removeClass("on");
  } 
}

function showtab_zmzb(k){
	$("#showtab_zmzb_area a").removeClass("on");
	$("#showtab_zmzb_area a").eq(k).addClass("on");
	$("#showtab_zmzb_area").parent().find(".tablist").eq(k).find("img[lz_src]").each(function(){
		$(this).attr("src",$(this).attr("lz_src")).removeAttr("lz_src");
	});
	$("#showtab_zmzb_area").parent().find(".tablist").hide();
	$("#showtab_zmzb_area").parent().find(".tablist").eq(k).show();
}

var index_rank = 1;
function page_goto(x) {
	$("#page_show").html(x);
	$("#rank_ul .licont").hide();
	$("#rank_ul .group_"+x).find("img[lz_src]").each(function(){
	  $(this).attr("src",$(this).attr("lz_src")).removeAttr("lz_src");													  
	});
	$("#rank_ul .group_"+x).show();
	index_rank = x;
}

function page_prev() {
	if (index_rank > 1) {
		index_rank--;
		page_goto(index_rank);
	}
}

function page_next() {
	if (index_rank < 2) {
		index_rank++;
		page_goto(index_rank);
	}
}

//装备大全功能切js
 var btype=0;
 var stype=0;
 var nclick=1;
 var zblist_zm4=$("#zblist_zm4 li");
 function showmore(){
  var s=nclick*27;
  var e=s+27;
  nclick=nclick+1;
  if(e>=zblist_zm4.length){
	e=zblist_zm4.length;
	$("#showmore").css("display","none");
  }else{
	$("#showmore").css("display","");  
  }
  for(var k=s;k<e;k++){
	zblist_zm4.eq(k).css("display","inline").find("img[lz_src]").attr("src",function(){
	    var imgurl=$(this).attr("lz_src");
		$(this).removeAttr("lz_src");
		return imgurl;
	});
  }
 }
 
 //小分类
 $("#stype  a").each(function(k){
	$(this).unbind().bind("click",function(){
		var rel=$(this).attr("rel");
		$("#stype  a").removeClass("on");
		$(this).addClass("on");
		stype=rel;
		nclick=0;
		if(stype==0){
		  zblist_zm4=$("#zblist_zm4 li");
		}else{
		  zblist_zm4=$("#zblist_zm4 li").filter("[stype='"+stype+"']");
		}
		$("#zblist_zm4 li").hide();
		if(zblist_zm4.length<=0){
			if(stype==34261){
			  alert("亲爱的造梦小勇士，游戏中宠物功能暂未开放，精彩内容，敬请期待！");
			}else{
			  alert("小编正在努力录入中，敬请期待！");
			}
		     btype=0;
			 stype=0;
			 nclick=0;
			 zblist_zm4=$("#zblist_zm4 li");
		}
		
		showmore();
		return false;
	});							
 });

//boss选项卡切换
var bossnp=1;
var bosstype=0;
var bindex=new Array();
var default_c='请输入你要查找的BOSS';
var blist=$("#bosslist li");
bindex[0]=0;
bindex[37]=1;
bindex[38]=2;
bindex[39]=3;
bindex[40]=4;
bindex[99]=5;
function bossmore(){
   var s1=bossnp*40;
   var e1=s1+40;
   bossnp=bossnp+1;
  var len=blist.length;
  if(len>40){$("#bossmore").css("display","");}else{$("#bossmore").css("display","none");}
  if(len<e1){e1=len;$("#bossmore").css("display","none");}
  $("#bosslist li").css("display","none");
  for(var j=0;j<e1;j++){
    blist.eq(j).show().find("img[lz_src]").attr("src",function(){
	   var src=$(this).attr("lz_src");
	   $(this).removeAttr("lz_src");
	   return src;
	});
  }
}
function gboss(kn){
 bossnp=0;
 bosstype=kn;
 $("#bkey").val(default_c);
 var jz= __get$(bindex,kn) ;
 $("#btab  a").attr("class","");
 $("#btab  a").eq(jz).attr("class","on");
  if(kn==0){
	 blist=$("#bosslist li");
  }else{
	blist=$("#bosslist li").filter("[rel="+bosstype+"]");  
  }
 bossmore();
}

function searchboss1(){
  var keyword=$("#bkey1").val();
  var keyword=$.trim(keyword);
  if(keyword=='' || keyword==default_c){
	 alert(default_c);$("#bkey").focus();
	 return false;
  }
 blist=$("#bosslist li").filter("[rel1*='"+keyword+"']");
 if(blist.length<=0){alert("您搜索的BOSS不存在，请输入正确的BOSS名称，才能搜索到哦！");$("#bkey").focus();return false;}
 $("#btab li a").attr("class","");
  bossmore();
}

